<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<screen-config xmlns="http://schemas.haulmont.com/cuba/screens.xsd">
    <include file="com/audimex/dashboard/screens.xml"/>
    <!--dashboard-->
    <screen id="dashboardBrowse"
            template="/com/audimex/dashboard/web/dashboard/frames/browse/dashboard-browse.xml"/>
    <screen id="dashboardEdit"
            template="/com/audimex/dashboard/web/dashboard/frames/editor/dashboard-edit.xml"/>
    <screen id="dashboardComponent"
            template="/com/audimex/dashboard/web/dashboard/ui_component/web-dashboard-frame.xml"/>
    <!--dashboard frames-->
    <screen id="paletteFrame"
            template="/com/audimex/dashboard/web/dashboard/frames/editor/palette/palette-frame.xml"/>
    <screen id="canvasFrame"
            template="/com/audimex/dashboard/web/dashboard/frames/canvas/canvas-frame.xml"/>
    <screen id="canvasEditorFrame"
            template="/com/audimex/dashboard/web/dashboard/frames/canvas/canvas-editor-frame.xml"/>
    <screen id="grid-dialog"
            template="/com/audimex/dashboard/web/dashboard/frames/editor/grid_creation_dialog/grid-creation-dialog.xml"/>
    <!--parameters-->
    <screen id="amdx$Parameter.browse"
            template="com/audimex/dashboard/web/parameter/parameter-browse.xml"/>
    <screen id="amxd$Parameter.edit"
            template="com/audimex/dashboard/web/parameter/parameter-edit.xml"/>
    <!--parameter value frames-->
    <screen id="simpleValueFrame"
            template="com/audimex/dashboard/web/parameter/frames/simple-value-frame.xml"/>
    <screen id="enumValueFrame"
            template="com/audimex/dashboard/web/parameter/frames/enum-value-frame.xml"/>
    <screen id="entitiesListValueFrame"
            template="com/audimex/dashboard/web/parameter/frames/entities-list-value-frame.xml"/>
    <screen id="entityValueFrame"
            template="com/audimex/dashboard/web/parameter/frames/entity-value-frame.xml"/>
    <screen id="entityValueWindow"
            template="com/audimex/dashboard/web/parameter/frames/entity-value-window.xml"/>
    <!--widget-->
    <screen id="widgetTemplateBrowse"
            template="com/audimex/dashboard/web/widget/widget-template-browse.xml"/>
    <screen id="widgetEdit"
            template="com/audimex/dashboard/web/widget/widget-edit.xml"/>
    <!--widget types-->
    <screen id="screenWidgetBrowse"
            template="com/audimex/dashboard/web/widget_types/screen/screen-widget-browse.xml"/>
    <screen id="screenWidgetEdit"
            template="com/audimex/dashboard/web/widget_types/screen/screen-widget-edit.xml"/>
    <screen id="lookupWidgetBrowse"
            template="com/audimex/dashboard/web/widget_types/lookup/lookup-widget-browse.xml"/>
    <screen id="lookupWidgetEdit"
            template="com/audimex/dashboard/web/widget_types/lookup/lookup-widget-edit.xml"/>
    <screen id="test"
            template="com/audimex/dashboard/web/screens/test.xml"/>
</screen-config>
