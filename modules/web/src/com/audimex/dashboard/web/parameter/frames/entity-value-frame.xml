<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.audimex.dashboard.web.parameter.frames.EntityValueFrame">
    <layout spacing="true">
        <hbox id="hBoxMetaClass"
              spacing="true">
            <label id="metaClassLabel"
                   width="85px"
                   value="msg://metaClass"
                   align="MIDDLE_LEFT"/>
            <lookupField id="metaClassLookup"
                         required="true"
                         width="300px"/>
        </hbox>
        <hbox id="hBoxEntity"
              spacing="true">
            <label id="entityLabel"
                   width="85px"
                   value="msg://entity"
                   align="MIDDLE_LEFT"/>
            <lookupField id="entitiesLookup"
                         required="true"
                         width="300px"/>
        </hbox>
        <hbox id="hBoxView"
              spacing="true">
            <label id="viewLabel"
                   width="85px"
                   value="msg://view"
                   align="MIDDLE_LEFT"/>
            <lookupField id="viewLookup"
                         required="true"
                         width="300px"/>
        </hbox>
    </layout>
</window>