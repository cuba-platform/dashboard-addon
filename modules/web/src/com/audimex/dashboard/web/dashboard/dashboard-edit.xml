<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.audimex.dashboard.web.dashboard.DashboardEdit"
        datasource="dashboardDs"
        focusComponent="fieldGroupLeft"
        messagesPack="com.audimex.dashboard.web.dashboard">
    <dsContext>
        <datasource id="dashboardDs"
                    class="com.audimex.dashboard.entity.Dashboard"
                    view="dashboard-view">
        </datasource>
    </dsContext>
    <dialogMode height="800"
                width="1200"
                resizable="true"/>
    <layout expand="dashboardDesignerVBox"
            spacing="true">
        <fieldGroup id="fieldGroupLeft"
                    datasource="dashboardDs">
            <column width="250px">
                <field id="title" property="title"/>
            </column>
            <column>
                <field id="entityType" property="entityType" custom="true"/>
            </column>
        </fieldGroup>
        <vbox id="dashboardDesignerVBox"/>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
