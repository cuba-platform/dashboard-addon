<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.audimex.dashboard.web.dashboard.frames.palette.PaletteFrame">
    <layout spacing="true"
            expand="scrollBox"
            width="100%"
            height="100%">
        <label id="paletteLabel"
               align="MIDDLE_CENTER"
               value="msg://palette"/>
        <scrollBox id="scrollBox"
                   scrollBars="vertical"
                   width="100%"
                   spacing="true">
            <vbox id="palette"
                  spacing="true"
                  height="100%"
                  width="100%">
            </vbox>
        </scrollBox>
    </layout>
</window>