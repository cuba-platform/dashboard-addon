<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.audimex.dashboard.web.palette.PaletteWindow"
        messagesPack="com.audimex.dashboard.web.palette">
    <dialogMode height="600"
                width="800"/>
    <layout>
        <hbox id="mainLayout"
              expand="dropLayout"
              height="100%"
              spacing="true"
              width="100%">
            <vbox expand="treeLayout"
                  height="100%"
                  spacing="true"
                  width="200px">
                <vbox id="containers"/>
                <hbox spacing="true">
                    <button id="removeComponent"
                            invoke="removeComponent"
                            enable="false"
                            caption="Remove"/>
                    <checkBox id="allowEdit"
                              align="MIDDLE_LEFT"
                              caption="Allow edit"/>
                </hbox>
                <vbox id="treeLayout"/>
            </vbox>
            <vbox id="dropLayout"
                  height="100%"
                  spacing="true"/>
        </hbox>
    </layout>
</window>
