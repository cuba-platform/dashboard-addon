<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<screen-config xmlns="http://schemas.haulmont.com/cuba/screens.xsd">
    <include file="/com/haulmont/addon/dashboard/screens.xml"/>
    <!--dashboard-->
    <screen id="dashboardBrowse"
            template="/com/haulmont/addon/dashboard/web/dashboard/frames/browse/persistent-dashboard-browse.xml"/>
    <screen id="dashboard-view"
            template="com/haulmont/addon/dashboard/web/dashboard/frames/browse/dashboard-view.xml"/>
    <screen id="dashboardEdit"
            template="/com/haulmont/addon/dashboard/web/dashboard/frames/editor/dashboard-edit.xml"/>
    <screen id="dashboardComponent"
            template="/com/haulmont/addon/dashboard/web/dashboard/frames/ui_component/web-dashboard-frame.xml"/>
    <!--dashboard frames-->
    <screen id="paletteFrame"
            template="/com/haulmont/addon/dashboard/web/dashboard/frames/editor/palette/palette-frame.xml"/>
    <screen id="canvasFrame"
            template="/com/haulmont/addon/dashboard/web/dashboard/frames/editor/canvas/canvas-frame.xml"/>
    <screen id="canvasEditorFrame"
            template="/com/haulmont/addon/dashboard/web/dashboard/frames/editor/canvas/canvas-editor-frame.xml"/>
    <screen id="grid-dialog"
            template="/com/haulmont/addon/dashboard/web/dashboard/frames/editor/grid_creation_dialog/grid-creation-dialog.xml"/>
    <screen id="weight-dialog"
            template="com/haulmont/addon/dashboard/web/dashboard/frames/editor/weight_dialog/weight-dialog.xml"/>
    <!--parameters-->
    <screen id="amdx$Parameter.browse"
            template="com/haulmont/addon/dashboard/web/parameter/parameter-browse.xml"/>
    <screen id="amxd$Parameter.edit"
            template="com/haulmont/addon/dashboard/web/parameter/parameter-edit.xml"/>
    <!--parameter value frames-->
    <screen id="simpleValueFrame"
            template="com/haulmont/addon/dashboard/web/parameter/frames/simple-value-frame.xml"/>
    <screen id="enumValueFrame"
            template="com/haulmont/addon/dashboard/web/parameter/frames/enum-value-frame.xml"/>
    <screen id="entitiesListValueFrame"
            template="com/haulmont/addon/dashboard/web/parameter/frames/entities-list-value-frame.xml"/>
    <screen id="entityValueFrame"
            template="com/haulmont/addon/dashboard/web/parameter/frames/entity-value-frame.xml"/>
    <screen id="entityValueWindow"
            template="com/haulmont/addon/dashboard/web/parameter/frames/entity-value-window.xml"/>
    <!--widget-->
    <screen id="widgetTemplateBrowse"
            template="com/haulmont/addon/dashboard/web/widget/widget-template-browse.xml"/>
    <screen id="widgetEdit"
            template="com/haulmont/addon/dashboard/web/widget/widget-edit.xml"/>
    <!--widget types-->
    <screen id="screenWidgetBrowse"
            template="com/haulmont/addon/dashboard/web/widget_types/screen/screen-widget-browse.xml"/>
    <screen id="screenWidgetEdit"
            template="com/haulmont/addon/dashboard/web/widget_types/screen/screen-widget-edit.xml"/>
    <screen id="lookupWidgetBrowse"
            template="com/haulmont/addon/dashboard/web/widget_types/lookup/lookup-widget-browse.xml"/>
    <screen id="lookupWidgetEdit"
            template="com/haulmont/addon/dashboard/web/widget_types/lookup/lookup-widget-edit.xml"/>
    <!--dashboard groups-->
    <screen id="amxd$DashboardGroup.browse"
            template="com/haulmont/addon/dashboard/web/dashboard_group/dashboard-group-browse.xml"/>
    <screen id="amxd$DashboardGroup.edit"
            template="com/haulmont/addon/dashboard/web/dashboard_group/dashboard-group-edit.xml"/>
    <!--widget template groups-->
    <screen id="amxd$WidgetTemplateGroup.browse"
            template="com/haulmont/addon/dashboard/web/widget_template_group/widget-template-group-browse.xml"/>
    <screen id="amxd$WidgetTemplateGroup.edit"
            template="com/haulmont/addon/dashboard/web/widget_template_group/widget-template-group-edit.xml"/>

    <screen id="userdispWidgetBrowse"
            template="com/haulmont/addon/dashboard/web/widget_types/displayer/userdisp-widget-browse.xml"/>
    <screen id="userdispWidgetEdit"
            template="com/haulmont/addon/dashboard/web/widget_types/displayer/userdisp-widget-edit.xml"/>
</screen-config>
